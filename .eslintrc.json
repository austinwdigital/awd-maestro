{
  "env": {
    "node": true,
    "es2022": true,
    "jest": true
  },
  "extends": [
    "eslint:recommended"
  ],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "no-unused-vars": "warn",
    "no-console": "off",
    "semi": [
      "error",
      "always"
    ],
    "no-eval": "error",
    "no-implied-eval": "error",
    "no-new-func": "error"
  }
}